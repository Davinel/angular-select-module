{
  "name": "angular-select-module",
  "version": "0.0.3",
  "author": "Eugene Popenko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ywein/angular-select-module"
  },
  "scripts": {
    "start": "npm run clean && npm run build:aot",
    "clean": "rimraf ./dist && rimraf ./aot && find ./src -name '*.js*' -type f -delete",
    "build:aot": "npm run buildStyles && ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "buildStyles": "postcss ./src/select/select.component.cssnext -p -o ./src/select/select.component.css -c ./postcss.config.js"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/platform-browser": "^4.0.1",
    "core-js": "^2.4.1",
    "install": "^0.8.8",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "copyfiles": "^1.2.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.2",
    "npm": "^4.4.4",
    "postcss": "^5.2.16",
    "postcss-cli": "^3.0.0",
    "postcss-cssnext": "^2.10.0",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
